{
  "name": "aqua-ai-public-edition",
  "version": "1.0.0",
  "description": "AI-powered water quality monitoring platform for India using public government data",
  "main": "index.js",
  "scripts": {
    "start": "cd backend && npm start",
    "postinstall": "cd backend && npm install && cd ../frontend && npm install --include=dev",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd backend && npm run dev",
    "build": "npm run frontend:build && npm run backend:build",
    "frontend:build": "cd frontend && npm run build",
    "backend:build": "cd backend && npm run build",
    "test": "npm run frontend:test && npm run backend:test",
    "frontend:test": "cd frontend && npm test",
    "backend:test": "cd backend && npm test",
    "ai:train": "cd ai-models && python train_model.py",
    "data:fetch": "cd data-pipeline && python fetch_data.py",
    "db:migrate": "cd database && npx knex migrate:latest",
    "db:seed": "cd database && npx knex seed:run",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "water-quality",
    "india",
    "pollution",
    "ai",
    "machine-learning",
    "environmental",
    "government-data",
    "hackathon"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.8.0"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "@supabase/supabase-js": "^2.97.0",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pg": "^8.18.0",
    "qs": "^6.14.2",
    "winston": "^3.11.0"
  }
}
